function sudo:dispatch:tail {
  while (( $# != 0 )); do
    case "$1" in
      -c|--bytes|-f|--follow|-n|--lines|--pid|-s|--sleep-interval) shift 2 ;;
      -*) shift ;;

      *) if [[ ! -r "$arg" ]] return 0 ;;
    esac
  done

  return 1
}

sudo:dispatch sudo:dispatch:tail tail

